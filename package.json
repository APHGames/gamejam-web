{
  "name": "gamejam-web",
  "author": "Adam Vesecky",
  "version": "0.0.1",
  "license": "MIT",
  "staticFiles": {
    "staticPath": "static"
  },
  "scripts": {
    "clean:output": "rimraf dist",
    "dev": "parcel view/index.html --no-cache",
    "deploy": "npm run clean:output && parcel build view/index.html --no-cache --public-url https://gamejam.aphgames.cz --no-source-maps",
    "dev_en": "parcel view/en/index.html --out-dir build",
    "deploy_en": "parcel build view/en/index.html --out-dir build --no-cache --public-url https://gamejam.aphgames.io/ --no-source-maps"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "parcel": "^2.7.0",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "rellax": "^1.12.1"
  }
}
